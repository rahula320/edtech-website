{
  "name": "edtech-website",
  "version": "1.0.0",
  "description": "A simple EdTech website with local authentication",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "init-db": "node server/scripts/init-db.js",
    "vercel-build": "cd client && npm install && npm run build",
    "build": "cd client && npm install && npm run build",
    "postinstall": "npm install --prefix client && npm install --prefix server"
  },
  "keywords": [
    "acmyx",
    "website",
    "education",
    "fullstack"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^2.0.22"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "sequelize": "^6.37.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rahula320/edtech-website.git"
  },
  "bugs": {
    "url": "https://github.com/rahula320/edtech-website/issues"
  },
  "homepage": "https://github.com/rahula320/edtech-website#readme",
  "engines": {
    "node": ">=14.0.0"
  }
}
