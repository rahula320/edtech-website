{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DATABASE_URL": "your-neon-postgresql-url",
    "SESSION_SECRET": "your-session-secret-here",
    "ADMIN_EMAIL": "your-admin-email",
    "ADMIN_PASSWORD": "your-admin-password"
  }
} 